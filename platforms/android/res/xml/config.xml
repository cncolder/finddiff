<?xml version='1.0' encoding='utf-8'?>
<widget id="com.vitarn.finddiff" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <name>FindDiff</name>
    <description>
        A children book to find difference between two images.
    </description>
    <author email="cncolder@gmail.com" href="http://vitarn.com">
        ViTarn.com
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="Orientation" value="landscape" />
    <preference name="Fullscreen" value="true" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="BackupWebStorage" value="none" />
    <icon src="res/icon.png" />
    <splash src="res/splash.png" />
</widget>
