{
    "apps": [
        {
            "env": {
                "DEBUG": "*",
                "DEBUG_COLORS": 1,
                "NODE_ENV": "development",
                "PORT": 3000
            },
            "env_production": {
                "DEBUG": "",
                "NODE_ENV": "production",
                "PORT": 80
            },
            "ignore_watch": [
                "[\\/\\\\]\\./",
                "node_modules",
                "public",
                "views"
            ],
            "instances": "1",
            "name": "finddiff",
            "node_args": "--harmony",
            "script": "server.js",
            "watch": false
        }
    ],
    "author": "colder",
    "bugs": {
        "url": "https://github.com/cncolder/finddiff/issues"
    },
    "dependencies": {},
    "description": "A children book to find difference between two images.",
    "devDependencies": {
        "babelify": "^7.3.0",
        "browserify": "^14.3.0",
        "debug": "^2.2.0",
        "gulp": "^3.8.11",
        "gulp-sourcemaps": "^2.6.0",
        "gulp-util": "^3.0.4",
        "koa": "^2.2.0",
        "koa-cors": "^0.0.16",
        "koa-logger": "^2.0.1",
        "koa-static": "^3.0.0",
        "lodash": "^4.17.4",
        "mocha": "^3.3.0",
        "partialify": "^3.1.3",
        "phaser": "^2.3.0",
        "semver": "^5.3.0",
        "vinyl-buffer": "^1.0.0",
        "vinyl-source-stream": "^1.1.0",
        "whatwg-fetch": "^2.0.3"
    },
    "homepage": "https://github.com/cncolder/finddiff",
    "license": "ISC",
    "main": "index.js",
    "name": "finddiff",
    "private": true,
    "repository": {
        "type": "git",
        "url": "https://github.com/cncolder/finddiff.git"
    },
    "scripts": {
        "loga": "./platforms/android/cordova/log | grep -i console",
        "logi": "./platforms/ios/cordova/log | grep FindDiff",
        "runa": "cordova run android --device",
        "runi": "cordova run ios --device",
        "test": "mocha --harmony"
    },
    "version": "1.0.0"
}
