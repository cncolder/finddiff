<?xml version="1.0" encoding="utf-8"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="com.vitarn.finddiff" version="1.0.0">
    <name>FindDiff</name>
    <description>
        A children book to find difference between two images.
    </description>
    <author email="cncolder@gmail.com" href="http://vitarn.com">
        ViTarn.com
    </author>
    <content src="index.html"/>
    <access origin="*"/>
    <preference name="Fullscreen" value="true"/>
    <preference name="Orientation" value="landscape"/>
    <preference name="DisallowOverscroll" value="true"/>
    <preference name="BackupWebStorage" value="local"/>
    <engine name="ios" spec="^3.8.0"/>
    <engine name="android" spec="^4.0.0"/>
</widget>
